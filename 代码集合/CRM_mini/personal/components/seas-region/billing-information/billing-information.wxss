/* personal/components/seas-region/billing-information/billing-information.wxss */

.clue-info-card {
  background: #ffffff;
  border-radius: 24rpx;
  padding: 24rpx;
  margin-top: 24rpx;
  font-size: 24rpx;
  color: #555555;
}

.clue-info-title {
  color: #333333;
  font-weight: bold;
  font-size: 28rpx;
  margin-bottom: 24rpx;
}

.clue-info-item {

  margin-bottom: 24rpx;
}

.clue-info-item :first-child {
  margin-right: 24rpx;
}

.sea-botton {
  border-top: 1rpx solid #EAEBEF;
  text-align: center;
  height: 91rpx;
  line-height: 91rpx;
  font-size: 28rpx;
  color: #0082FC;
}

.user-info {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.user-top {
  display: flex;
  align-items: center;
}

.user-img {
  width: 64rpx;
  height: 64rpx;
  border-radius: 50%;

}

.user-name {
  font-size: 28rpx;
  color: #333333;
}

.creted-time {
  font-size: 24rpx;
  color: #B4B4B4;
}

.empty-page {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  margin-top: 50%;
}

.empty-page>image {
  width: 320rpx;
  height: 224rpx;
}

.empty-page>text:nth-of-type(1) {
  margin: 12rpx 0;
  color: #555555;
  font-size: 24rpx;
}

.follow-desc-item {
  margin-bottom: 15rpx;
}

.location-img-sty {
  color: #0082FC;
  margin-left: 10rpx;
}